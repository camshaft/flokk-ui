<h2 data-i18n-bind="'My Account' | i18n:'account'"></h2>
<div class="media">
  <a class="pull-left" href="#">
    <img class="media-object" data-ng-src="{{account.avatar}}" />
  </a>
  <div class="media-body">
    <h3 data-ng-bind="account.name"></h3>
    <p data-ng-bind="account.email"></p>
  </div>
  <div class="cart">
    <h4>Cart</h4>
    <div data-ng-repeat="offer in cart.offer" data-ng-bind="offer | json"></div>
  </div>
</div>