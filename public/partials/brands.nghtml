<div class="row-fluid">
  <ul class="thumbnails items">
    <li class="span4 item" data-ng-repeat="brand in body.vendors">
      <div class="thumbnail">

        <a href="/brands/{{brand.title | slugify}}/{{brand.href | param}}">
          <div class="image" data-ng-style="{'background-image': 'url('+brand.logo+')'}" alt=""></div>
          <h4 data-ng-bind="brand.title"></h4>
        </a>

      </div>
    </li>
  </ul>
</div>
