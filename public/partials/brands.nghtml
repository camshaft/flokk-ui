<div class="row-fluid">
  <ul class="thumbnails">
    <li class="span4" data-ng-repeat="brand in body.vendors">
      <div class="thumbnail">

        <a href="/brands/{{brand.href | param}}">
          <img class="image" data-ng-src="{{brand.thumbnail}}" alt="">
          <h4 data-ng-bind="brand.title"></h4>
        </a>

      </div>
    </li>
  </ul>
</div>