<div class="row-fluid">
  <ul class="thumbnails">
    <li class="span4" data-ng-repeat="vendor in body.vendors">
      <div class="thumbnail">

        <a href="/vendors/{{vendor.href | param}}">
          <img class="image" data-ng-src="{{vendor.thumbnail}}" alt="">
          <h4 data-ng-bind="vendor.title"></h4>
        </a>

      </div>
    </li>
  </ul>
</div>